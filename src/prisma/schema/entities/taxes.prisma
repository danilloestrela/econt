model Taxes {
    id                 Int                  @id @default(autoincrement())
    amount             String
    name               String
    description        String
    currency           Currencies
    taxesRemunerations TaxesRemunerations[]
    taxesSummaries     TaxesSummaries[]
    created_at         DateTime             @default(now())
    updated_at         DateTime             @updatedAt

    @@map("taxes")
}
